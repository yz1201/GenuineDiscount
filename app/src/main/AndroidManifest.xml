<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tyz1201.mytablayout">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo_114"
        android:label="雪人"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">

        <activity
            android:name=".activity.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activity.MainActivity" />
        <activity android:name=".activity.BrandSingleGoodsActivity" />
        <activity android:name=".activity.LandingActivity" />
        <activity android:name=".activity.GoodsDetialActivity" />
        <activity android:name=".activity.GenuineDetailsActivity" />

    </application>

</manifest>
